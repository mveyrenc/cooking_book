# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

# Ignore uploaded files in development.
/storage/*
!/storage/.keep

/public/assets
/public/system
.byebug_history

# Ignore master key for decrypting credentials and more.
/config/master.key

# Ignore node vendors
node_modules

# Ignore tests results
/coverage
